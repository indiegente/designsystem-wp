{
  "strategy": "modular",
  "dataLayer": {
    "enabled": true,
    "provider": "native",
    "events": {
      "page_view": true,
      "component_view": true,
      "interaction": true,
      "form_submit": true
    }
  },
  "integrations": {
    "plugins": {
      "monsterinsights": {
        "enabled": false,
        "compatibility_mode": true
      },
      "site_kit": {
        "enabled": false,
        "compatibility_mode": true
      }
    },
    "gtm": {
      "enabled": false,
      "container_id": "",
      "data_layer_name": "dataLayer"
    },
    "native": {
      "enabled": true,
      "ga4": {
        "measurement_id": "G-ABC123DEF4",
        "enabled": true
      },
      "facebook_pixel": {
        "pixel_id": "",
        "enabled": false
      }
    }
  },
  "page_templates": {
    "page-carreras": {
      "page_view": "page_view_carreras",
      "events": [
        {
          "name": "hero_cta_click",
