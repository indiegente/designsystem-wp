{
  "page-carreras": {
    "file": "page-carreras.php",
    "title": "Carreras",
    "description": "Explora nuestras carreras técnicas y programas especializados",
    "wordpressData": {
      "postType": "carrera",
      "query": {
        "numberposts": -1,
        "post_status": "publish"
      }
    },
    "components": [
      {
        "name": "hero-section",
        "props": {
          "title": "Nuestras Carreras",
          "subtitle": "Descubre tu potencial creativo con nuestras carreras técnicas",
          "cta_text": "Explorar carreras",
          "background_image": ""
        },
        "seo": {
          "title": "Carreras Técnicas | Toulouse Lautrec",
          "description": "Explora nuestras carreras técnicas y programas especializados en diseño, tecnología y creatividad.",
          "keywords": "carreras técnicas, diseño, tecnología, creatividad, Toulouse Lautrec",
          "ogImage": "",
          "canonical": "/carreras"
        },
        "analytics": {
          "pageView": "page_view_carreras",
          "events": [
            {
              "name": "hero_cta_click",
              "category": "engagement",
              "action": "click",
              "label": "hero_cta_carreras"
            },
            {
              "name": "course_card_click",
              "category": "engagement", 
              "action": "click",
              "label": "course_card_view"
            }
          ]
        }
      },
      {
        "name": "course-card",
        "props": {
          "title": "",
          "description": "",
          "image": "",
          "link": "",
          "link_text": "Ver carrera"
        },
        "dataSource": {
          "type": "post",
          "postType": "carrera",
          "query": {
            "numberposts": -1,
            "post_status": "publish"
          },
          "mapping": {
            "title": {
              "source": "post_title",
              "type": "native"
            },
            "description": {
              "source": "post_excerpt",
              "type": "native"
            },
            "image": {
              "source": "post_thumbnail_url",
              "type": "native"
            },
            "link": {
              "source": "post_permalink",
              "type": "native"
            },
            "link_text": {
              "source": "Ver carrera",
              "type": "static"
            }
          }
        }
      },
      {
        "name": "testimonials",
        "props": {
          "title": "Lo que dicen nuestros estudiantes",
          "subtitle": "Testimonios de éxito de nuestros egresados"
        },
        "dataSource": {
          "type": "post",
          "postType": "testimonio",
          "query": {
            "numberposts": 6,
            "post_status": "publish"
          },
          "aggregation": {
            "mode": "collect",
            "dataStructure": {
              "name": {
                "source": "post_title",
                "type": "native"
              },
              "role": {
                "source": "post_excerpt",
                "type": "native"
              },
              "content": {
                "source": "post_content",
                "type": "native"
              },
              "rating": {
                "source": "meta_rating",
                "type": "native"
              },
              "avatar": {
                "source": "meta_avatar",
                "type": "acf"
              },
              "course": {
                "source": "meta_course",
                "type": "acf"
              }
            },
            "defaultValues": {
              "rating": 5
            }
          }
        }
      }
    ]
  },
  "page-contacto": {
    "file": "page-contacto.php",
    "title": "Contacto",
    "description": "Ponte en contacto con nosotros",
    "wordpressData": {
      "postType": "page"
    },
    "components": [
      {
        "name": "hero-section",
        "props": {
          "title": "Contáctanos",
          "subtitle": "Estamos aquí para ayudarte en tu camino creativo",
          "cta_text": "Enviar mensaje",
          "background_image": ""
        },
        "seo": {
          "title": "Contacto | Toulouse Lautrec",
          "description": "Ponte en contacto con nosotros. Estamos aquí para ayudarte en tu camino creativo.",
          "keywords": "contacto, Toulouse Lautrec, información, ayuda",
          "ogImage": "",
          "canonical": "/contacto"
        },
        "analytics": {
          "pageView": "page_view_contacto",
          "events": [
            {
              "name": "contact_form_submit",
              "category": "conversion",
              "action": "submit",
              "label": "contact_form"
            }
          ]
        }
      }
    ]
  },
  "page-test-showcase": {
    "file": "page-test-showcase.php",
    "title": "Test Showcase - Validación Completa",
    "description": "Página para probar todos los managers del sistema",
    "wordpressData": {
      "postType": "page"
    },
    "components": [
      {
        "name": "test-showcase",
        "props": {
          "title": "Test Showcase - Validación de Managers",
          "description": "Componente completo que ejercita todos los managers del sistema WordPress",
          "testImages": [
            {"src": "https://via.placeholder.com/300x200/FF6B35/FFFFFF?text=Asset+Test+1", "alt": "Asset Manager Test 1"},
            {"src": "https://via.placeholder.com/300x200/F7931E/FFFFFF?text=Asset+Test+2", "alt": "Asset Manager Test 2"},
            {"src": "https://via.placeholder.com/300x200/FFD23F/000000?text=Analytics+Test", "alt": "Analytics Manager Test"},
            {"src": "https://via.placeholder.com/300x200/06FFA5/000000?text=SEO+Test", "alt": "SEO Manager Test"}
          ],
          "testData": {
            "analytics": ["page_view", "component_view", "interaction", "form_submit"],
            "seo": {"schema": "WebPage", "structured": true},
            "performance": {"lazyLoading": true, "preload": true}
          },
          "enableAnalytics": true
        },
        "dataSource": {
          "type": "page",
          "wordpressData": {
            "fields": {
              "title": {
                "source": "post_title",
                "type": "native"
              },
              "description": {
                "source": "post_excerpt",
                "type": "native"
              },
              "testImages": {
                "source": "meta_test_images",
                "type": "acf"
              },
              "testData": {
                "source": "meta_test_data",
                "type": "acf"
              },
              "enableAnalytics": {
                "source": "meta_enable_analytics",
                "type": "acf"
              }
            },
            "defaultValues": {
              "enableAnalytics": true,
              "testImages": [
                { "src": "https://via.placeholder.com/300x200/FF6B35/FFFFFF?text=Test+1", "alt": "Imagen de prueba 1" },
                { "src": "https://via.placeholder.com/300x200/F7931E/FFFFFF?text=Test+2", "alt": "Imagen de prueba 2" }
              ]
            }
          }
        },
        "seo": {
          "title": "Test Showcase - Validación Completa | Toulouse Lautrec",
          "description": "Página de prueba para validar todos los managers del sistema de generación WordPress",
          "keywords": "test, showcase, managers, wordpress, validation",
          "canonical": "/test-showcase"
        },
        "analytics": {
          "pageView": "page_view_test_showcase",
          "events": [
            {
              "name": "test_component_load",
              "category": "testing",
              "action": "load",
              "label": "test_showcase_component"
            }
          ]
        }
      }
    ]
  }
}
